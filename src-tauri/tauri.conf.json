{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "r-shell",
  "version": "0.6.4",
  "identifier": "com.aiden.r-shell",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "r-shell",
        "width": 1920,
        "height": 1080,
        "fullscreen": false,
        "maximized": true,
        "resizable": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/GOODBOY008/r-shell/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEyREE0MDUyMUUzNzIzOTUKUldTVkl6Y2VVa0RhRXBTZUNuMzgrQ3ljR0hVUDRDWHd5R0dPcVQwVzlnY0pIb3pFcXV5OFJ2MlIK"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
